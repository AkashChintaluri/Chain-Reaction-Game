public class Functions
{
    public void print(main2[][])
    {
        int k = 0;
        System.out.println("\t\t\t\t   1  2 3  4 5  6 7 8");
        for(int j = 0; j <8; j++)
        {
            System.out.print("\t\t\t\t");
            System.out.print((char)(k+65)+"  ");
            for(int h = 0; h<8; h++)
            {
                if(main2[j][h]=='e')
                    System.out.print((char)9724+" ");
                else
                    System.out.print(main[j][h]+ " ");
            }
            k = k +1;
            System.out.println();
        }
    }
} 